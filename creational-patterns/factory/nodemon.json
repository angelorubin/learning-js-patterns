{
	"restartable": "rs",
	"colours": true,
	"watch": ["src/index.js"],
	"stdin": true,
	"ignore": [".git", "node_modules/**/node_modules"],
	"verbose": true,
	"execMap": {
		"js": "node --inspect --harmony"
	},
	"signal": "SIGUSR2",
	"stdout": true,
	"env": {
		"NODE_ENV": "development"
	},
	"ext": "js,json"
}
